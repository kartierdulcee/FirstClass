{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "firstclassfranchise.com" }],
      "destination": "https://www.firstclassfranchise.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
